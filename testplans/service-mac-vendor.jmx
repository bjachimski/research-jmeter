          <stringProp name="script">// SETUP
def a = vars.getObject(&apos;customAssert&apos;)	// reusable utilities
def root = a.getRoot(prev);			// root json object from previous response
def msg = &quot;&quot;;						// failure Message accumulator

// CHECK CONNECTIVITY
a.check200(prev);

// CHECK DEVNAV RESPONSE SHAPE
msg += a.checkShape(root, msg);

// CHECK GENERAL DOMAIN SHAPE
def map = [:];
map[&quot;result.company&quot;] = root.result.company;
map[&quot;result.mac_prefix&quot;] = root.result.mac_prefix;
map[&quot;result.address&quot;] = root.result.address;
map[&quot;result.start_hexpany&quot;] = root.result.start_hex;
map[&quot;result.end_hex&quot;] = root.result.end_hex;
map[&quot;result.type&quot;] = root.result.type;
msg += a.checkMandatoryFields(map, msg);

// WRAPUP
a.processFailureMessage(msg);</stringProp>
